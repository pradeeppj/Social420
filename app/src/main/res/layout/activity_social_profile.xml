<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" tools:context=".social_profile">




    <LinearLayout
        android:id="@+id/topbar"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/menufinal"
            android:id="@+id/imageView4"
            />

        <ImageView

            android:layout_width="wrap_content"
            app:srcCompat="@drawable/tab_nofill"
            android:layout_height="wrap_content"
            android:id="@+id/togglenew"
            android:layout_weight="1.03"
            android:textOff=""
            android:textOn=""
            android:layout_gravity="center_vertical"/>



        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"

            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/profileee1"
            android:id="@+id/imageView5"
            android:visibility="visible"/>


    </LinearLayout>


    <ImageView
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="#00FFFFFF"
        android:id="@+id/click"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:textColor="#9F9FAB"
        android:backgroundTint="#f5f5fa"
        android:text="Rebecca Jane"
        android:ems="10"
        android:ellipsize="end"
        android:singleLine="true"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:id="@+id/profile_name"
        android:layout_below="@+id/topbar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <GridLayout
        android:id="@+id/images_grid1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:rowCount="1"
        android:padding="0dp"
        android:layout_below="@+id/images_grid"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <ImageButton
            android:id="@+id/image4"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="4"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_columnWeight="1"

            android:background="#FFFFFF"
            app:srcCompat="@drawable/image4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            />

        <ImageButton

            android:id="@+id/image5"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="5"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/image5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />

        <ImageButton

            android:id="@+id/image6"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="6"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:background="#FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/image6"



            />


    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:padding="0dp"
        android:orientation="horizontal"
        android:rowCount="3"
        android:id="@+id/images_grid"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <ImageButton

            android:id="@+id/image3"
            android:layout_columnSpan="1"
            android:layout_rowSpan="2"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="2"
            android:layout_row="0"
            android:background="#FFFFFF"
            android:layout_column="0"
            app:srcCompat="@drawable/imagebig"

            android:layout_columnWeight="2"
            android:layout_rowWeight="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/image1"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="1"
            android:layout_row="0"
            android:background="#FFFFFF"
            android:layout_column="2"
            app:srcCompat="@drawable/image3"
            android:padding="5dp"

            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:layout_width="wrap_content" />

        <ImageButton

            android:id="@+id/image2"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"
            android:background="#FFFFFF"
            android:layout_gravity="fill_horizontal|fill_vertical"
            android:text="1"
            android:layout_row="1"
            android:layout_column="2"
            app:srcCompat="@drawable/image2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:layout_width="wrap_content" />


    </GridLayout>


</RelativeLayout>